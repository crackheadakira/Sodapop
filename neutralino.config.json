{
  "applicationId": "js.sodapop.player",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/dist",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log",
    "filesystem.*",
    "custom.*"
  ],
  "modes": {
    "window": {
      "title": "Sodapop",
      "width": 1200,
      "height": 800,
      "minWidth": 800,
      "minHeight": 600,
      "icon": "/public/logo.png",
      "enableInspector": true,
      "exitProcessOnClose": true
    }
  },
  "cli": {
    "binaryName": "Sodapop",
    "resourcesPath": "/dist/",
    "extensionsPath": "/",
    "clientLibrary": "/public/neutralino.js",
    "binaryVersion": "nightly",
    "clientVersion": "nightly",
    "frontendLibrary": {
      "patchFile": "/index.html",
      "devUrl": "http://localhost:5173"
    }
  }
}